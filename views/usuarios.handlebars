<h1>Usuários</h1>
<a href="/usuarios">Usuários</a>
<table border="1">
    <tr>
        <td>Nickname</td>
        <td>Nome</td>
    </tr>
    <tr>
    {{#each usuarios}}
      <tr>
        <td>{{this.id}}</td>
        <td>{{this.nickname}}</td>
        <td>{{this.nome}}</td>
        <td>
            <a href="/usuarios/{{this.id}}/atualizar">Atualizar</a>
        </td>
      </tr>
    {{/each}}

    </tr>
</table>


